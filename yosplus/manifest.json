{
   "background": {
      "matches": [ "http://forums.somethingawful.com/*", "https://api.twitter.com/*" ],
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "exclude_matches": [ "http://forums.somethingawful.com/account.php?action=loginform*" ],
      "js": [ "js/jquery.min.js", "js/jquery-migrate.min.js", "js/rangyinputs-jquery-src.js", "js/rangy/rangyinputs-jquery-src.js", "js/jquery.lazyload.js", "contentscript.js", "js/jquery.ba-bbq.js", "js/highlight.js", "js/pasteimage.js", "js/jqueryui/jquery-ui.js", "js/dropzone.js", "js/SAT.js", "js/inview.js" ],
      "matches": [ "http://forums.somethingawful.com/*", "https://api.twitter.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Performing CPR on the Something Awful forums",
   "icons": {
      "128": "images/san128.png",
      "16": "images/san16.png",
      "48": "images/san48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "SA Needful",
   "page_action": {
      "default_icon": "images/san128.png",
      "default_popup": "popup.html",
      "default_title": "SA Needful"
   },
   "permissions": [ "storage", "declarativeContent" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.996",
   "web_accessible_resources": [ "images/*.png", "audio/*.wav", "css/*.css", "js/*.js", "fonts/*.*" ]
}
